{
  "schema": "./src/schema.ts",
  "generates": {
    "./src/types.ts": {
      "plugins": [
        "typescript",
        "typescript-operations",
        "typescript-resolvers"
      ],
      "config": {
        "contextType": "./context#DataSourceContext",
        "mappers": {
          "Track": "./models#TrackModel",
          "Author": "./models#AuthorModel"
        }
      }
    }
  }
}